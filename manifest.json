{
  "manifest_version": 3,
  "name": "Jhud",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "web_accessible_resources": [
    {
      "resources": ["page-inject.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
